        <font>
            <Font size="14.0"/>
        </font>
    </Button>
    <PasswordField fx:id="password" layoutX="217.0" layoutY="200.0" promptText="password">
        <font>
            <Font size="14.0"/>
        </font>
    </PasswordField>
    <TextField fx:id="login" layoutX="217.0" layoutY="126.0" prefHeight="30.0" prefWidth="173.0" promptText="login">
        <font>
          <Font size="14.0"/>
        </font>
    </TextField>
</AnchorPane>
